<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <csp>
            <mode>
                <storefront>
                    <report_only>1</report_only>
                    <frame-ancestors>
                        <policy_id>frame-ancestors</policy_id>
                        <self>1</self>
                        <inline>1</inline>
                        <eval>0</eval>
                        <dynamic>0</dynamic>
                    </frame-ancestors>
                </storefront>
                <admin>
                    <report_only>1</report_only>
                    <frame-ancestors>
                        <policy_id>frame-ancestors</policy_id>
                        <self>1</self>
                        <inline>0</inline>
                        <eval>0</eval>
                        <dynamic>0</dynamic>
                    </frame-ancestors>
                </admin>
            </mode>
        </csp>
        <payment>
            <totalprocessing_opp>
                <active>0</active>
                <allowed_brand_types>MASTERDEBIT,MASTER,VISA,VISADEBIT,VISAELECTRON,AMEX</allowed_brand_types>
                <allowspecific>0</allowspecific>
                <debug>0</debug>
                <debug_log_filepath>var/log/totalprocessing/debug.log</debug_log_filepath>
                <error_log_filepath>var/log/totalprocessing/error.log</error_log_filepath>
                <default_locale></default_locale>
                <can_authorize>1</can_authorize>
                <can_authorize_vault>1</can_authorize_vault>
                <can_cancel>1</can_cancel>
                <can_capture>1</can_capture>
                <can_capture_vault>1</can_capture_vault>
                <can_deny_payment>1</can_deny_payment>
                <can_edit>1</can_edit>
                <can_refund>1</can_refund>
                <can_refund_partial_per_invoice>1</can_refund_partial_per_invoice>
                <can_void>1</can_void>
                <can_use_checkout>1</can_use_checkout>
                <can_use_internal>0</can_use_internal>
                <environment>sandbox</environment>
                <is_gateway>1</is_gateway>
                <model>TotalProcessingFacade</model>
                <order_status>pending</order_status>
                <payment_action>authorize_capture</payment_action>
                <payment_methods>cards</payment_methods>
                <require_cardholder>0</require_cardholder>
                <scheduler>0</scheduler>
                <sort_order>1</sort_order>
                <style_options>card</style_options>
                <iframe_style><![CDATA[border: none;]]></iframe_style>
                <style_options_custom_iframe_css><![CDATA[.wpwl-brand-card {float: right;}]]></style_options_custom_iframe_css>
                <style_options_custom_iframe_js><![CDATA[
//let self = this;
//console.log("Custom Javascript Script");
//console.log(self);

// Example custom Field order :
// documentation https://totalprocessing.docs.oppwa.com/tutorials/integration-guide/advanced-options#fieldorderincardform

//$(".wpwl-group-expiry").after($(".wpwl-group-cardNumber"));]]></style_options_custom_iframe_js>
                <style_options_default_css><![CDATA[.wpwl-group-cardHolder {display:none !important;}]]></style_options_default_css>
                <title>Pay By Credit or Debit Card</title>
                <live_api_url><![CDATA[https://oppwa.com]]></live_api_url>
                <sandbox_api_url><![CDATA[https://test.oppwa.com]]></sandbox_api_url>
                <live_payment_url><![CDATA[https://oppwa.com]]></live_payment_url>
                <sandbox_payment_url><![CDATA[https://test.oppwa.com]]></sandbox_payment_url>
                <payment_btn_text>Place Order</payment_btn_text>
                <vault_enabled>0</vault_enabled>
                <sandbox_entity_id backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <sandbox_access_token backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <live_entity_id backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <live_access_token backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
            </totalprocessing_opp>
            <totalprocessing_opp_vault>
                <model>TotalProcessingVaultFacade</model>
                <title>Stored Cards</title>
                <instant_purchase>
                    <supported>1</supported>
                </instant_purchase>
            </totalprocessing_opp_vault>
            <totalprocessing_opp_applepay>
                <active>0</active>
                <allowed_brand_types>amex,mastercard,visa</allowed_brand_types>
                <can_authorize>1</can_authorize>
                <can_cancel>1</can_cancel>
                <can_capture>1</can_capture>
                <can_deny_payment>1</can_deny_payment>
                <can_edit>1</can_edit>
                <can_refund>1</can_refund>
                <can_use_checkout>1</can_use_checkout>
                <can_use_internal>0</can_use_internal>
                <can_void>1</can_void>
                <environment>sandbox</environment>
                <is_gateway>1</is_gateway>
                <live_api_url><![CDATA[https://tpap2.totalprocessing.com]]></live_api_url>
                <live_merchant_id_domain_association><![CDATA[]]></live_merchant_id_domain_association>
                <live_register_url><![CDATA[https://applepay-v2.totalprocessing.com/service/registerWcMerchant.php]]></live_register_url>
                <live_shopper_endpoint><![CDATA[oppwa.com]]></live_shopper_endpoint>
                <model>TotalProcessingApplePayFacade</model>
                <payment_action>authorize_capture</payment_action>
                <sandbox_api_url><![CDATA[https://tpap2.totalprocessing.com]]></sandbox_api_url>
                <sandbox_merchant_id_domain_association><![CDATA[]]></sandbox_merchant_id_domain_association>
                <sandbox_register_url><![CDATA[https://applepay-v2.totalprocessing.com/service/registerWcMerchant.php]]></sandbox_register_url>
                <sandbox_shopper_endpoint><![CDATA[test.oppwa.com]]></sandbox_shopper_endpoint>
                <title>Apple Pay (Total Processing)</title>
            </totalprocessing_opp_applepay>
        </payment>
    </default>
</config>
